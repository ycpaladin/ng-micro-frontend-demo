{
  "name": "smes-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "//01": "配置开发模式启动",
    "start:main": "ng serve --host 0.0.0.0 --proxy-config ./proxy.conf.json --live-reload false",
    "start:workbench": "ng serve workbench --proxy-config ./proxy.conf.json --deploy-url /app-workbench/ --host 0.0.0.0 --port 4201 --live-reload false",
    "start:system-mgmt": "ng serve system-mgmt --proxy-config ./proxy.conf.json --deploy-url /app-system/ --host 0.0.0.0 --port 4202 --live-reload false",
    "start:business-mgmt": "ng serve business-mgmt --proxy-config ./proxy.conf.json --deploy-url /app-business/ --host 0.0.0.0 --port 4203 --live-reload false",
    "start": "npx npm-run-all --parallel start:*",
    "//02": "编译配置",
    "build:main": "ng build --prod",
    "build:workbench": "ng build workbench --prod --deploy-url /app-workbench/ ",
    "build:system-mgmt": "ng build system-mgmt --prod --deploy-url /app-system/",
    "build:business-mgmt": "ng build business-mgmt --prod --deploy-url /app-business/",
    "build": "npx npm-run-all --parallel build:*",
    "//03": "配置子应用单独启动（开发模式）",
    "alone:system-mgmt": "ng run system-mgmt:serveAlone --proxy-config ./proxy.conf.json --port 4202",
    "alone:business-mgmt": "ng run business-mgmt:serveAlone --proxy-config ./proxy.conf.json --port 4203",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "upgrade:lib": "yarn add @cy/lib@latest --registry=http://10.6.32.13:4873/",
    "postinstall": "ngcc",
    "postuninstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.4",
    "@angular/common": "~11.2.4",
    "@angular/compiler": "~11.2.4",
    "@angular/core": "~11.2.4",
    "@angular/forms": "~11.2.4",
    "@angular/platform-browser": "~11.2.4",
    "@angular/platform-browser-dynamic": "~11.2.4",
    "@angular/router": "~11.2.4",
    "@date-fns/upgrade": "^1.0.3",
    "@techiediaries/ngx-qrcode": "^9.1.0",
    "@types/lodash": "^4.14.168",
    "@worktile/planet": "^10.0.0",
    "jsbarcode": "^3.11.3",
    "mathjs": "^9.3.2",
    "ng-zorro-antd": "11.4.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^11.1.1",
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular/cli": "~11.2.3",
    "@angular/compiler-cli": "~11.2.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "webpack-assets-manifest": "3.1.1"
  }
}
